{
  "name": "kairos-flow",
  "version": "1.0.0",
  "description": "Production-ready CLI framework for building granular multi-agent AI systems.",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "bin": {
    "kairos": "./bin/kairos.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "node dist/cli.js",
    "dev": "ts-node src/cli.ts",
    "lint": "eslint src/**/*.ts",
    "format": "prettier --write src/**/*.ts",
    "test": "jest",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "ai",
    "llm",
    "multi-agent",
    "prompt-engineering",
    "framework",
    "agents",
    "workflow",
    "automation"
