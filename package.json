  "author": "Javier Baal",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/JavierBaal/KairosFlow.git"
  },
  "dependencies": {
    "@anthropic-ai/sdk": "^0.70.1",
    "chalk": "^4.1.2",
    "commander": "^11.1.0",
    "dotenv": "^16.6.1",
    "express": "^5.1.0",
    "fs-extra": "^11.2.0",
    "inquirer": "^8.2.6",
    "open": "^11.0.0",
    "openai": "^6.9.1",
    "ora": "^5.4.1",
    "socket.io": "^4.8.1",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/fs-extra": "^11.0.4",
    "@types/inquirer": "^9.0.7",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.0",
    "jest": "^30.2.0",
    "prettier": "^3.1.0",
    "ts-jest": "^29.4.5",
    "ts-node": "^10.9.1",
    "typescript": "^5.3.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
